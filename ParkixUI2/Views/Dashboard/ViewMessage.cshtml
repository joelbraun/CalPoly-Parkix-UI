
@{
    ViewData["Title"] = "ViewMessage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="right_col" role="main">
    <div class="">

        <div class="clearfix"></div>

        <div class="row">
            <div class="col-md-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Message: <small>@ViewData["MessageSubject"] - @ViewData["MessageDateTime"]</small></h2>
                        <div class="clearfix"></div>
                    </div>
                    <div>
                        @Html.Raw(ViewData["MessageData"])
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




